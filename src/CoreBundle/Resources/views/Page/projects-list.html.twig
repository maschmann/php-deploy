<tbody id="asm-projects-list">
{% for project in projects %}
    <tr>
        <td>{{ project.name|raw }}</td>
        <td>{{ project.playbook|raw }}</td>
        <td>{{ project.inventory|raw }}</td>
        <td>{{ 'last.deployment' }}</td>
        <td>{{ 'last successfull depl' }}</td>
        <td>
            <div class="btn-group btn-group-xs">
                <button title="{{ 'Edit'|trans }}" type="button" class="btn btn-default btn-xs asm-edit-btn" data-link="{{ path('core_project.form', {'id': project.id}) }}" aria-label="{{ 'edit'|trans }}">
                    <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
                </button>
                <button title="{{ 'Delete'|trans }}" type="button" class="btn btn-default btn-xs asm-delete-btn"  data-link="{{ path('core_project.delete') }}" data-id="{{ project.id }}" aria-label="{{ 'delete'|trans }}">
                    <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
                </button>
            </div>
        </td>
    </tr>
{% endfor %}
</tbody>
